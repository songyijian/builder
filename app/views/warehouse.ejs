<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
	<title> 仓库项目-myServ </title>
	<link rel="stylesheet" href="/css/style.css">
	<script>
		document.write( `<link rel="shortcut icon" href="${location.origin}/favicon.png">` )
	</script>
	<script src="/js/jquery.js"></script>
	<style>
		.panel{ overflow: hidden;}
		.panel>p{ width: 48%; margin: 0.25rem 1%; float: left; }
		.panel>p>span{  margin-right:0.5rem }
	</style>
</head>
<body>
	<div class="panel">
		<a href="/" >返回首页</a>
	</div>

	<div class="panel">
		<% if(err){ %>
			<p class="errtxt"><%= err %></p>
		<% }else{ %>
			<% for(var j=0; j<data.length; j++){%>
				<% if(data[j]!=='.DS_Store' && data[j]!=='.babelrc'){ %>
					<p>
						<span class="tagbtn" onclick="ideOpenItem('<%= pathy %>/<%= data[j] %>')">IDE</span>
						<% if(data[j].indexOf('.')<0){ %>
								<a href="<%= data[j] %>" class="dir_a"><%= data[j] %></a>
							<% }else{ %>
								<a href="<%= data[j] %>" target="_blank" class="file_a" ><%= data[j] %></a>
						<% } %>
					</p>
				<% } %>
			<% } %>
		<% } %>
	</div>  

	<script>
	function ideOpenItem(url){
		if(!url){
			alert('路径不存在')
			return 
		}
		$.post('/shell/ide_open_item', {url} , (res) => {
			console.log(res)
		})
	}</script>
</body>
</html>
